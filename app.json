{
  "expo": {
    "name": "Prayer Reminder",
    "slug": "prayer-reminder",
    "scheme": "pray",
    "ios": {
      "supportsTablet": false,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to calculate accurate prayer times.",
        "NSUserTrackingUsageDescription": "We only use notifications to remind you of prayer times.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.prayer-reminder"
    },
    "plugins": ["expo-notifications", "expo-task-manager", "expo-background-fetch"],
    "android": {
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "VIBRATE",
        "WAKE_LOCK",
        "SCHEDULE_EXACT_ALARM"
      ],
      "useNextNotificationsApi": true,
      "package": "com.example.prayerreminder"
    },
    "extra": {
      "eas": {
        "projectId": "c6692e49-ca84-4599-8e10-ce1802f77413"
      }
    }
  }
}
